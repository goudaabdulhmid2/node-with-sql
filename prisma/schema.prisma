// You can have multiple genrator
generator client {
  provider = "prisma-client-js"
}

// Yoc can only have one date source and must havea provider
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
